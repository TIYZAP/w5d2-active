<table class="table table-inverse">

    <thead>
        <tr>
            <% @colums.each do |colum| %>
            <th>
                <%= colum.upcase %>
            </th>
            <%end%>
        </tr>

    </thead>
    <tbody>
        <% @items.each do |item| %>
        <tr>
            <% item.attributes.values.each do |value| %>
            <td>
                <%= value %>
            </td>
            <%end%>
        </tr>
        <%end%>
    </tbody>
</table>
